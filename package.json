    "@nuxtjs/pwa": "^3.0.0-0",
    "@nuxtjs/sitemap": "^2.4.0",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "gsap": "^3.3.3",
    "lazysizes": "^5.2.2",
    "locomotive-scroll": "^3.5.4",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.15.0",
    "swiper": "^6.8.1",
    "vue-awesome-swiper": "^4.1.1",
    "vuex-persist": "^2.2.0"
  },
